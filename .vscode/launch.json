    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "WebSever",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "webserver"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Gulp typescript",
            "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
            "args": [
                "typescript"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "program": "${workspaceFolder}/index.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}