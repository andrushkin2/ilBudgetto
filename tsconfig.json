
{
    "include": [
        "server/wwwroot/**/*.ts"
    ],
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es2015",
        "module": "commonjs",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noStrictGenericChecks": false,
        "allowJs": false,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "forceConsistentCasingInFileNames": true,
        "allowUnreachableCode": false,
        "noUnusedLocals": true
    }
}